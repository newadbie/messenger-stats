generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Session {
    id           String   @id @default(cuid())
    sessionToken String   @unique
    userId       String
    expires      DateTime
}

model User {
    id        String @id @default(cuid())
    firstName String @map("first_name")
    lastName  String @map("last_name")
}
